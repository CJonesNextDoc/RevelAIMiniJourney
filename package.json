{
  "name": "revelai-mini-journey",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
  "dev": "tsx watch src/index.ts",
  "build": "tsc -p tsconfig.json",
  "typecheck": "tsc --noEmit",
  "prepare": "husky install",
  "start": "node dist/index.js",
  "test": "jest --runInBand",
  "lint": "eslint \"src/**/*.ts\" \"tests/**/*.ts\" --ext .ts",
  "test:ci": "jest --runInBand --colors",
  "test:single": "jest -i --colors",
  "test:watch": "jest --watch"
  },
  "dependencies": {
    "better-sqlite3": "^11.5.0",
    "fastify": "^5.0.0",
    "uuid": "^11.1.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
  "husky": "^8.0.0",
  "lint-staged": "^14.0.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.7.4",
    "@types/uuid": "^10.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
  "ts-node": "^10.9.2",
  "tslib": "^2.8.0",
  "tsx": "^4.19.0",
  "typescript": "^5.6.2",
  "eslint": "^8.50.0",
  "@typescript-eslint/parser": "^6.9.0",
  "@typescript-eslint/eslint-plugin": "^6.9.0"
  }
}
